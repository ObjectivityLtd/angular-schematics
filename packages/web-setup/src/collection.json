{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Configures useful aspects of Angular project.",
      "factory": "./ng-add/index",
      "schema": "./ng-add/schema.json",
      "aliases": ["install"]
    },
    "ts-import-path": {
      "description": "Improves long relative paths import",
      "factory": "./ts-import-path/index#tsImportPath",
      "schema": "./ts-import-path/schema.json",
      "private": true
    },
    "structure": {
      "description": "Creates recommended project structure",
      "factory": "./structure/index#structure",
      "schema": "./structure/schema.json",
      "private": true
    },
    "scss-import": {
      "description": "Improves long relative paths import",
      "factory": "./scss-import/index#scssImport",
      "schema": "./scss-import/schema.json",
      "private": true
    }
  }
}
